  <main class="flex flex-col items-center justify-content-center">
    <section class="flex items-center justify-center px-3 bg-gray-100 py-10 md:py-20 w-full" id="hero">
      <div class="align-middle mx-auto">
        <div class="flex items-center text-center">
          <div class="flex flex-col space-y-4">
            <div class="text-5xl md:text-7xl xl:text-8xl font-bold text-shadow">
              <h1 data-aos="fade-right">Success!</h1>
            </div>
            <div class="text-lg" data-aos="fade-right" data-aos-delay="200">
              <h2 class="text-3xl font-semibold">Here are your purchased products.</h2>
              <p class="text-base">(Your products were also sent to your email. Check your your inbox and spam!)</p>
              <div class="py-6 space-y-4">
                <div class="flex justify-between items-center md:w-2/3 lg:w-1/2 mx-auto">
                  <div class="text-left">
                    <p class="">Order number:</p>
                    <p class="">Plan:</p>
                    <p class="">Quantity:</p>
                  </div>
                  <div class="text-right font-bold">
                    <p>
                      <%= orderNum %>
                    </p>
                    <p>
                      <%= product %>
                    </p>
                    <p>
                      <%= quantity %>
                    </p>
                  </div>
                </div>
                <div class="md:w-3/4 mx-auto p-3 bg-gray-200 font-mono text-sm">
                  <% /* Should be plural if multiple products */ %>
                    <% if (purchasedCodes.length> 1) { %>
                      <br>Here are your products:
                      <% } else { %>
                        <br>Here is your product:
                        <% } %>

                          <% /* Create new tag for each product */ %>
                            <% for (const code of purchasedCodes) { %>
                              <br> <b>
                                <%= code %>
                              </b>
                              <% } %>
                </div>
              </div>
            </div>
            <div class="text-2xl" data-aos="fade-right" data-aos-delay="200">
              Please leave a review of your products on <a
                href="https://twitter.com/danielservicess/status/1370753110931570693" class="link">Twitter</a> and <a
                href="https://www.instagram.com/p/Br1jQr8H7yY/" class="link">Instagram</a>.
            </div>
            <div class="grid grid-cols-1 md:grid-cols-2 gap-3 mx-auto" data-aos="fade-right" data-aos-delay="400">
              <a class="px-4 w-full py-3 lg:py-6 flex items-center btn" href="index.html">Back to Store
                &nbsp;&nbsp;<svg aria-hidden="true" focusable="false" data-prefix="far" data-icon="shopping-basket"
                  class="h-5" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 576 512">
                  <path fill="currentColor"
                    d="M564 192h-72.902L362.286 40.457c-8.583-10.099-23.729-11.327-33.83-2.743-10.099 8.584-11.327 23.731-2.742 33.83L428.102 192H147.899L250.287 71.543c8.584-10.099 7.356-25.246-2.743-33.83s-25.246-7.355-33.83 2.743L84.901 192H12c-6.627 0-12 5.373-12 12v24c0 6.627 5.373 12 12 12h18.667l27.584 198.603C61.546 462.334 81.836 480 105.794 480h364.412c23.958 0 44.248-17.666 47.544-41.397L545.333 240H564c6.627 0 12-5.373 12-12v-24c0-6.627-5.373-12-12-12zm-93.794 240H105.794L79.127 240h417.745l-26.666 192zM312 296v80c0 13.255-10.745 24-24 24s-24-10.745-24-24v-80c0-13.255 10.745-24 24-24s24 10.745 24 24zm112 0v80c0 13.255-10.745 24-24 24s-24-10.745-24-24v-80c0-13.255 10.745-24 24-24s24 10.745 24 24zm-224 0v80c0 13.255-10.745 24-24 24s-24-10.745-24-24v-80c0-13.255 10.745-24 24-24s24 10.745 24 24z">
